{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/ref/verify",
      "destination": "/referee-evaluation-page.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
