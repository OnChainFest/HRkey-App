{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rootDirectory": "HRkey",
  "redirects": [
    { "source": "/auth",          "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/auth/",         "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/auth.html",     "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/Auth.html",     "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/AUTH.html",     "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/webdapp/auth.html", "destination": "/WebDapp/auth.html", "permanent": false }
  ]
}
