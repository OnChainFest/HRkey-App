{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/",     "destination": "/index.html" },
    { "source": "/auth", "destination": "/auth.html"  },
    { "source": "/app",  "destination": "/app.html"   },
    { "source": "/upgrade", "destination": "/app.html"},
    { "source": "/(.*)", "destination": "/$1"         }
  ]
}
