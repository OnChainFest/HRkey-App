# Supabase (Public - safe for client-side)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase (Private - server-side only)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Coinbase Developer Platform
NEXT_PUBLIC_CDP_API_KEY=your-cdp-api-key

# Blockchain - Base Sepolia
NEXT_PUBLIC_CONTRACT_ADDRESS=0xYourContractAddressHere
NEXT_PUBLIC_BASE_SEPOLIA_ID=84532

# App URL (auto-configured by Vercel, but can override)
NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app
NEXT_PUBLIC_VERCEL_URL=your-project.vercel.app

# EmailJS (Email service) - Get from https://www.emailjs.com/
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your-public-key-here
NEXT_PUBLIC_EMAILJS_SERVICE_ID=service_abc123
NEXT_PUBLIC_EMAILJS_TEMPLATE_INVITE=template_invite_xxx
NEXT_PUBLIC_EMAILJS_TEMPLATE_COMPLETED=template_completed_xxx
NEXT_PUBLIC_EMAILJS_TEMPLATE_DIGEST=template_digest_xxx
DIGEST_TO_EMAIL=your-email@example.com

# Stripe (Payments)
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
PRICE_ID_LIFETIME=price_your_price_id_here
