{
  "version": 2,
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    { "source": "/auth", "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/auth/", "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/auth.html", "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/Auth.html", "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/AUTH.html", "destination": "/WebDapp/auth.html", "permanent": false },
    { "source": "/app", "destination": "/WebDapp/app.html", "permanent": false },
    { "source": "/app/", "destination": "/WebDapp/app.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=60" }
      ]
    }
  ]
}

