{
  "version": 2,
  "framework": null,
  "installCommand": "true",
  "buildCommand": "true",
  "outputDirectory": "public",
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    { "source": "/auth",       "destination": "/WebDapp/auth.html",  "permanent": false },
    { "source": "/auth.html",  "destination": "/WebDapp/auth.html",  "permanent": false },
    { "source": "/Auth.html",  "destination": "/WebDapp/auth.html",  "permanent": false },
    { "source": "/AUTH.html",  "destination": "/WebDapp/auth.html",  "permanent": false },

    { "source": "/app.html",   "destination": "/WebDapp/app.html",   "permanent": false },
    { "source": "/index.html", "destination": "/WebDapp/index.html", "permanent": false }
  ]
}
