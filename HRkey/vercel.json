{
  "buildCommand": "cd HRkey && npm run build",
  "devCommand": "cd HRkey && npm run dev",
  "installCommand": "cd HRkey && npm install",
  "framework": "nextjs",
  "outputDirectory": "HRkey/.next",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/(.*)", "destination": "/HRkey/$1" }
  ]
}
